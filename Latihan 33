class Vehicle:
    def __init__(self,brand,model):
        self.brand = brand
        self.model = model

    def move(self):
        print("move!")

class Car(Vehicle):
    pass

class Boat(Vehicle):
    def move(self):
        print("Sai1!")

class Plane(Vehicle):
    def move(self):
        print("Fly!")

car1 = Car("Ford", "Mustang")       #create a Car object
boat1 = Boat("Ibiza", "Touring 20") #create a Boat object
plane1 = Plane("Boeing", "747")     #create a Plane object

for x in (car1, boat1, plane1):
    print(x.brand)
    print(x.model)
    x.move()
